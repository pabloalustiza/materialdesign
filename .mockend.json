{
  "User": { 
    "nombre": { "regexp": "[A-Z][a-z]{5,10}" },
    "password": { "regexp": "[a-z]{5,10}[a-z]{5}" },
    "userImg": {
      "regexp": "https://picsum\\.photos/seed/[0-9]{5}/600/600"
    },
    "email": {
      "regexp": "[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
    }
  },
  "Dispositivo": {
    "serialNumber": {
        "int": {
          "min": 1000,
          "max": 9999
        }
      },
    "fechaInicio": {
      "dateTime": {
        "min": "2010-01-01T00:00:00Z",
        "max": "2020-12-31T23:59:59Z"
      }
    },
    "ph": {
        "int": {
          "min": 0,
          "max": 14
        }
      },
    "temperatura": {
        "int": {
          "min": 0,
          "max": 45
        }
      }
  }
}
